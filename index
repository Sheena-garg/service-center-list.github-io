<script>
  $(function() {
    $(".citySel li").sort(sort_li).appendTo('.citySel');
    function sort_li(a, b) {
      return ($(b).data('value')) < ($(a).data('value')) ? 1 : -1;
    }
  });
  $(".choose-opt").click(function(){
    $("#myInput").show();
    $("ul.citySel").slideToggle();
  })
  $(".citySel li").click(function(){
    $("ul.citySel, #myInput").hide();
    var city = $(this).data("value");
    var cityText = $(this).text();
    $("#myText").text(cityText);
    $(".faqs-section .faq-block").each(function(){
      if($(this).data("id") == city) {
        $(this).show().siblings(".faq-block").hide();
      }
    })
  })
  $(document).click(function (e) {
    e.stopPropagation();
    var container = $(".drop-menu");
    if (container.has(e.target).length === 0) {
        $('.citySel, #myInput').hide();
    }
  }) 
  function filterFunction() {
    var input, filter, ul, li, a, i;
    input = document.getElementById("myInput");
    filter = input.value.toUpperCase();
    div = document.getElementById("myDropdown");
    a = div.getElementsByTagName("li");
    for (i = 0; i < a.length; i++) {
      txtValue = a[i].textContent || a[i].innerText;
      if (txtValue.toUpperCase().indexOf(filter) > -1) {
        a[i].style.display = "";
      } else {
        a[i].style.display = "none";
      }
    }
  }
</script>

</body>
</html>
